    <command:details>
      <command:name>Initialize-Module</command:name>
      <command:verb>Initialize</command:verb>
      <command:noun>Module</command:noun>
      <maml:description>
        <maml:para>Check if module is installed or needs update</maml:para>
      </maml:description>
